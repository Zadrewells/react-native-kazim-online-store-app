{
	"expo": {
		"name": "Simple store",
		"slug": "simplestore",
		"privacy": "public",
		"sdkVersion": "32.0.0",
		"platforms": [
			"ios",
			"android"
		],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"androidStatusBarColor": "#ffffff",
		"androidStatusBar": {
			"barStyle": "dark-content",
			"backgroundColor": "#ffffff"
		}
	}
}